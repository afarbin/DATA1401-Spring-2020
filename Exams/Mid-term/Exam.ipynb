    "def find_x_90(x):\n",
    "    h = histogram(x, len(x))\n",
    "    N = float(len(x))\n",
    "    x_90 = h[1][1] # second to lowest edge by default\n",
    "    for i in range(1, len(h[0])):\n",
    "        if cumulative_sum(h[0][:i])[-1] < 0.9*N:\n",
    "            x_90 = h[1][i+1]\n",
    "        else:\n",
    "            break\n",
    "    return x_90\n",
