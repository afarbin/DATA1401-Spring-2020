   "source": [
    "def outer_product(lst1, lst2):\n",
    "    lst3 = [[0 for i in range(len(lst2))] for j in range(len(lst1))]\n",
    "    for i in range(len(lst1)):\n",
    "        for j in range(len(lst2)):\n",
    "            lst3[i][j] = lst1[i]*lst2[j]\n",
    "    return lst3\n",
    "\n",
    "tl1 = [1, 2, 3, 4, 5]\n",
    "tl2 = [6, 7, 8]\n",
    "op = outer_product(tl1, tl2)\n",
    "for l in op:\n",
    "    print(l)"
