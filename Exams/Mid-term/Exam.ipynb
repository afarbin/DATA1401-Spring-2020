    "def first_alphabetically(lst):\n",
    "    # return min(lst)\n",
    "    first = lst[0]\n",
    "    for state in lst:\n",
    "        if first.upper() > state.upper():\n",
    "            first = state\n",
    "    return first\n",
    "\n",
    "\n",
    "print(first_alphabetically(states))"
   ]
